<?xml version="1.0" encoding="UTF-8"?>
<database>

    <global>
        <table name="feeds">
            <field type="bigint( 10 ) NOT NULL AUTO_INCREMENT PRIMARY KEY">id</field>
            <field type="varchar(255)">project</field>
            <field type="varchar(2)">lang</field>
            <field type="text NULL">feedsites</field>
            <field type="varchar(255) NULL">feedtype</field>
            <field type="varchar(255) NULL">feedName</field>
            <field type="varchar(255) NULL">feedDescription</field>
            <field type="int(100) NULL">feedlimit</field>
        </table>
    </global>

</database>